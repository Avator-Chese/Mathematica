Cell[BoxData[
 RowBox[{
  RowBox[{"nearestNeighborsAndSurfaces", "[", "crystalStructure_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "coordinates", ",", "nearestDistance", ",", "nearestDistanceMidPoints", 
      ",", "intercept"}], "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{"(*", 
     RowBox[{
     "Giving", " ", "a", " ", "set", " ", "of", " ", "points", " ", "in", " ",
       "RL"}], "*)"}], "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"coordinates", "=", 
      RowBox[{"Flatten", "[", 
       RowBox[{
        RowBox[{"Table", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"q", " ", 
            RowBox[{"crystalStructure", "[", 
             RowBox[{"[", "1", "]"}], "]"}]}], "+", 
           RowBox[{"r", " ", 
            RowBox[{"crystalStructure", "[", 
             RowBox[{"[", "2", "]"}], "]"}]}], "+", 
           RowBox[{"s", " ", 
            RowBox[{"crystalStructure", "[", 
             RowBox[{"[", "3", "]"}], "]"}]}]}], ",", 
          RowBox[{"{", 
           RowBox[{"q", ",", 
            RowBox[{"-", "3"}], ",", "3"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"r", ",", 
            RowBox[{"-", "3"}], ",", "3"}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"s", ",", 
            RowBox[{"-", "3"}], ",", "3"}], "}"}]}], "]"}], ",", "2"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Finding", " ", "the", " ", "nearext", " ", "neighbors", " ", "of", " ",
        "the", " ", "origin", " ", "except", " ", "the", " ", "origin", " ", 
       "itself"}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"nearestDistance", "=", 
      RowBox[{"RankedMin", "[", 
       RowBox[{
        RowBox[{"Flatten", "[", 
         RowBox[{"Table", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"coordinates", "[", 
              RowBox[{"[", 
               RowBox[{"m", ",", "1"}], "]"}], "]"}], "^", "2"}], "+", 
            RowBox[{
             RowBox[{"coordinates", "[", 
              RowBox[{"[", 
               RowBox[{"m", ",", "2"}], "]"}], "]"}], "^", "2"}], "+", 
            RowBox[{
             RowBox[{"coordinates", "[", 
              RowBox[{"[", 
               RowBox[{"m", ",", "3"}], "]"}], "]"}], "^", "2"}]}], ",", 
           RowBox[{"{", 
            RowBox[{"m", ",", "1", ",", 
             RowBox[{"Length", "[", "coordinates", "]"}]}], "}"}]}], "]"}], 
         "]"}], ",", "2"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Finding", " ", "the", " ", "mid"}], "-", 
       RowBox[{
       "points", " ", "of", " ", "the", " ", "origin", " ", "and", " ", "its",
         " ", "nearest", " ", "neighbors"}]}], "*)"}], "\[IndentingNewLine]", 
     RowBox[{"nearestDistanceMidPoints", "=", 
      RowBox[{
       RowBox[{"Select", "[", 
        RowBox[{"coordinates", ",", 
         RowBox[{
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "1", "]"}], "]"}], "^", "2"}], "+", 
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}], "^", "2"}], "+", 
            RowBox[{
